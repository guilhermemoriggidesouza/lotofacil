generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Bids {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    updatedAt DateTime @updatedAt
    userId    String?  @db.ObjectId
}

model user {
    id    String  @id @default(auto()) @map("_id") @db.ObjectId
    username String?
    password String
}
