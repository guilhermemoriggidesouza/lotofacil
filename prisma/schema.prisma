generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}


model Bids {
    id        String    @id @default(auto()) @map("_id") @db.ObjectId
    winner    Boolean
    createdAt DateTime  @default(now())
    numbers   Json[]
}

model user {
    id       String  @id @default(auto()) @map("_id") @db.ObjectId
    username String?
    password String
}
