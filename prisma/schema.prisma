generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["orderByRelation"]
}

generator json {
    provider = "prisma-json-types-generator"
}

datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

model Bids {
    id        String   @id @default(auto()) @map("_id") @db.ObjectId
    winner    Boolean
    createdAt DateTime @default(now())
    ///[NumberBids]
    numbers   Json[]
}

model user {
    id       String  @id @default(auto()) @map("_id") @db.ObjectId
    username String?
    password String
}
